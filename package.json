{
  "name": "hono-todo",
  "type": "module",
  "scripts": {
    "ai": "node ./scripts/copyFiles.js",
    "dev": "concurrently \"node ./scripts/generateWranglerToml.js\" \"vite --port 8080\" \"cross-env LOCAL_DB_PATH=$(find .wrangler/state/v3/d1/miniflare-D1DatabaseObject -type f -name '*.sqlite' -print -quit) drizzle-kit studio\"",
    "build": "node ./scripts/prodGenerateWranglerToml.js && vite build --mode client && vite build",
    "deploy": "$npm_execpath run build && wrangler pages deploy && wrangler d1 migrations apply hono-todo --remote",
    "db:create:prod": "wrangler d1 create hono-todo",
    "db:generate": "drizzle-kit generate",
    "db:apply": "wrangler d1 migrations apply hono-todo --local",
    "db:apply:prod": "wrangler d1 migrations apply hono-todo --remote",
    "db:studio:prod": "drizzle-kit studio"
  },
  "dependencies": {
    "@capacitor/preferences": "^6.0.2",
    "@hono/oidc-auth": "^1.1.0",
    "@hono/zod-validator": "^0.2.2",
    "@ionic/react": "^8.3.1",
    "@ionic/react-router": "^8.3.1",
    "@tanstack/react-query": "^5.56.2",
    "@tanstack/react-query-persist-client": "^5.56.2",
    "@types/react-router-dom": "^5.3.3",
    "drizzle-orm": "^0.33.0",
    "hono": "^4.6.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^5.3.4"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.1",
    "@cloudflare/workers-types": "^4.20240529.0",
    "@hono/vite-build": "^1.0.0",
    "@hono/vite-cloudflare-pages": "^0.4.2",
    "@hono/vite-dev-server": "^0.15.2",
    "@types/react": "^18.3.6",
    "@types/react-dom": "^18.3.0",
    "better-sqlite3": "^11.3.0",
    "clipboardy": "^4.0.0",
    "concurrently": "^9.0.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.24.2",
    "vite": "^5.4.5",
    "vite-plugin-static-copy": "^1.0.6",
    "vite-tsconfig-paths": "^5.0.1",
    "wrangler": "^3.57.2"
  },
  "version": "0.0.1"
}
